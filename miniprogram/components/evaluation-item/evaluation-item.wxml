<view class="evaluationItem">
    <view class="header">
        <view class="userInfo">
            <text>用户: {{evaluation.lastFiveOpenId}} </text>
            <text> \n 评分: {{evaluation.rating}} </text>
            <view wx:if="{{evaluation.alelmengCertified}}">
                <text class="tag">爱乐盟认证</text>
            </view>
        </view>
        <text class="dateInfo">日期: {{evaluation.date}}</text>
    </view>
    <view class="comment">
        <view>{{evaluation.comment}}</view>
    </view>
</view>
